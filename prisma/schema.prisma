generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Upload {
  id         String   @id @default(cuid())
  uploadAt   DateTime @default(now())
  expiresAt  DateTime
  filename   String
  url        String   @unique
  type       String
  r2Key      String
  domain     String
}